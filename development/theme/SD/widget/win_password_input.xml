<?xml version="1.0" encoding="GB2312" standalone="no"?>
<widget style="default" name="win_password_input" class="window">
    <property>
        <rect>[210,150,300,141]</rect>
        <forecolor>[#CEE8FF,#212021,#CEE8FF]</forecolor>
        <backcolor>[#FF00FF,#FF00FF,#FF00FF]</backcolor>
    </property>
    <signal>
        <create>win_password_input_create</create>
        <destroy>win_password_input_destroy</destroy>
        <keypress>win_password_input_keypress</keypress>
    </signal>
    <children>
        <widget style="default" name="win_password_input_rect_image" class="image">
            <property>
                <rect>[0,0,300,141]</rect>
                <forecolor>[#CEE8FF,#212021,#CEE8FF]</forecolor>
                <backcolor>[#515151,#515151,#515151]</backcolor>
                <mode>multiple</mode>
                <img></img>
                <lt_img>DS_MESSAGE_LEFT.bmp</lt_img>
                <rt_img>DS_MESSAGE_RIGHT.bmp</rt_img>
                <lb_img>DS_MESSAGE_LEFT.bmp</lb_img>
                <rb_img>DS_MESSAGE_RIGHT.bmp</rb_img>
                <l_img>DS_MESSAGE_LEFT.bmp</l_img>
                <r_img>DS_MESSAGE_RIGHT.bmp</r_img>
                <t_img>DS_MESSAGE_MIDDLE.bmp</t_img>
                <b_img>DS_MESSAGE_MIDDLE.bmp</b_img>
            </property>
        </widget>
        <widget style="default" name="win_password_input_tip" class="text">
            <property>
                <rect>[11,2,280,34]</rect>
                <forecolor>[#bbff00,#bbff00,#bbff00]</forecolor>
                <backcolor>[#ff00ff,#ff00ff,#ff00ff]</backcolor>
                <font>Arial</font>
                <string>Please Input  password</string>
                <format>static</format>
            </property>
        </widget>
        <widget style="default" name="win_password_input_edit" class="edit">
            <property>
                <rect>[78,58,150,34]</rect>
                <forecolor>[#debe42,#040404,#040404]</forecolor>
                <backcolor>[#debe42,#debe42,#debe42]</backcolor>
                <font>Arial</font>
                <alignment>hcentre|vcentre</alignment>
                <intaglio>*</intaglio>
                <maxlen>4</maxlen>
                <format>edit_digit</format>
            </property>
            <signal>
                <keypress>win_password_input_edit_keypress</keypress>
                <reach_end>win_password_input_reach_end</reach_end>
            </signal>
        </widget>
    </children>
</widget>
